    <form-bean name="historyAddForm" type="com.yourcompany.struts.form.HistoryAddForm" />
    <form-bean name="historyDelForm" type="com.yourcompany.struts.form.HistoryDelForm" />
    <form-bean name="leaveGetForm" type="com.yourcompany.struts.form.LeaveGetForm" />
    <form-bean name="leaveDelForm" type="com.yourcompany.struts.form.LeaveDelForm" />
    <form-bean name="loginForm" type="com.yourcompany.struts.form.LoginForm" />

  </form-beans>

  <global-exceptions />
  <global-forwards />
  <action-mappings >
    <action
      attribute="newsPageForm"
      name="newsPageForm"
      path="/template/news"
      scope="request"
      type="com.yourcompany.struts.action.NewsAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="leaveForm"
      input="/template/leave.html"
      name="leaveForm"
      path="/template/leave"
      scope="request"
      type="com.yourcompany.struts.action.LeaveAction">
      <set-property property="cancellable" value="true" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="intrAndHisForm"
      input="/template/historic.html"
      name="intrAndHisForm"
      path="/template/history"
      scope="request"
      type="com.yourcompany.struts.action.HistoryAction">
      <set-property property="cancellable" value="true" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="newsDetailForm"
      name="newsDetailForm"
      path="/template/newsDetail"
      scope="request"
      type="com.yourcompany.struts.action.NewsDetailAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="beautifulForm"
      name="beautifulForm"
      path="/template/beautiful"
      scope="request"
      type="com.yourcompany.struts.action.BeautifulAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
