      scope="request"
      type="com.yourcompany.struts.action.IntroduceAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="specialForm"
      name="specialForm"
      path="/template/special"
      scope="request"
      type="com.yourcompany.struts.action.SpecialAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="delicacyForm"
