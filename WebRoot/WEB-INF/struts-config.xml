<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
  <data-sources />
  <form-beans >
    <form-bean name="newsForm" type="com.yourcompany.struts.form.NewsForm" />
    <form-bean name="leaveForm" type="com.yourcompany.struts.form.LeaveForm" />
    <form-bean name="historyForm" type="com.yourcompany.struts.form.HistoryForm" />
    <form-bean name="newsDetailForm" type="com.yourcompany.struts.form.NewsDetailForm" />
    <form-bean name="newsPageForm" type="com.yourcompany.struts.form.NewsPageForm" />
    <form-bean name="beautifulForm" type="com.yourcompany.struts.form.BeautifulForm" />
    <form-bean name="introduceForm" type="com.yourcompany.struts.form.IntroduceForm" />
    <form-bean name="intrAndHisForm" type="com.yourcompany.struts.form.IntrAndHisForm" />
    <form-bean name="specialForm" type="com.yourcompany.struts.form.SpecialForm" />
    <form-bean name="delicacyForm" type="com.yourcompany.struts.form.DelicacyForm" />
    <form-bean name="newsAddForm" type="com.yourcompany.struts.form.NewsAddForm" />
    <form-bean name="newsDelForm" type="com.yourcompany.struts.form.NewsDelForm" />
    <form-bean name="beautifulDelForm" type="com.yourcompany.struts.form.BeautifulDelForm" />
    <form-bean name="beautifulAddForm" type="com.yourcompany.struts.form.BeautifulAddForm" />
    <form-bean name="delicacyDelForm" type="com.yourcompany.struts.form.DelicacyDelForm" />
