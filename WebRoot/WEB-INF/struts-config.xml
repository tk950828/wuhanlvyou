      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="newsDelForm"
      name="newsDelForm"
      path="/template/newsDel"
      scope="request"
      type="com.yourcompany.struts.action.NewsDelAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="beautifulDelForm"
      name="beautifulDelForm"
      path="/template/beautifulDel"
      scope="request"
      type="com.yourcompany.struts.action.BeautifulDelAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
