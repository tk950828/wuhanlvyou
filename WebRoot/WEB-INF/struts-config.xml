      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="delicacyAddForm"
      name="delicacyAddForm"
      path="/template/delicacyAdd"
      scope="request"
      type="com.yourcompany.struts.action.DelicacyAddAction">
      <set-property property="cancellable" value="false" />
      <forward name="success" path="/successJsp.jsp" />
    </action>
    <action
      attribute="historyAddForm"
      name="historyAddForm"
      path="/template/historyAdd"
      scope="request"
      type="com.yourcompany.struts.action.HistoryAddAction">
      <set-property property="cancellable" value="false" />
